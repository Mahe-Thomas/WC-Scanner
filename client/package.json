{
  "name": "wcscanner",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "files": [
    "/**/*",
    "*.js"
  ],
  "build": {
    "appId": "com.example.electron-cra",
    "asar": true,
    "asarUnpack": [
      "core/**/*",
      "assets/**/*"
    ],
    "files": [
      "core/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "*.js",
      "index.html"
    ],
    "directories":{
      "buildResources": "build"
    }
  },
  "scripts": {
    "start": "npm install && electron .",
    "dist": "build --dir"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "bootstrap-material-design": "^4.1.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  }
}
